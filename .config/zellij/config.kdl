keybinds {
    normal {
        // Vim uses Ctrl-t for jumping backwards with tags (e.g., definitions)
        unbind "Ctrl t"
        bind "Ctrl y" { SwitchToMode "tab"; }
    }
}

theme "one-dark"

themes {
    one-dark {
        fg "#A8B4CC"
        bg "#120E10"
        black "#000000"
        red "#E06C75"
        green "#98C379"
        yellow "#E5C07B"
        blue "#61AFEF"
        magenta "#C678DD"
        cyan "#56B6C2"
        white "#ABB2BF"
        orange "#D19A66"
    }
}

